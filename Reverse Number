public class ReverseNumber {
    public static void main(String[] args) {
    Scanner Sc=new Scanner(System.in);
    int t=Sc.nextInt();
    for(int i=1; i<=t; i++)
    {
        int n=Sc.nextInt();
        int ans=0;
    while(n!=0){
        int last_digit=n%10;
        ans=ans*10+last_digit;
        n=n/10;
    }
    System.out.println(ans);
}
}}
